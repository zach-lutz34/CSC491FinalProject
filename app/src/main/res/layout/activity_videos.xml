<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Videos"
    android:padding="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="27dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Videos"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"></TextView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="30dp"
            app:cardMaxElevation="30dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="48dp">

            <TextView
                android:id="@+id/txtView2"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="50dp"
                android:paddingTop="5dp"
                android:text="Bench Press"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="206dp"
                tools:layout_editor_absoluteY="16dp" />

            <VideoView
                android:id="@+id/vidView2"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="225dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="218dp"
                tools:layout_editor_absoluteY="47dp" />

            <VideoView
                android:id="@+id/vidView1"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="47dp" />

            <TextView
                android:id="@+id/txtView"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="18dp"
                android:paddingTop="5dp"
                android:text="1 Arm 1 Leg Dumbbell RDL"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView

            android:id="@+id/cardView2"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginTop="14dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="30dp"
            app:cardMaxElevation="30dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp">

            <TextView
                android:id="@+id/txtView3"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="45dp"
                android:paddingTop="5dp"
                android:text="Barbell Deadlift"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="206dp"
                tools:layout_editor_absoluteY="16dp" />

            <VideoView
                android:id="@+id/vidView4"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="225dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="218dp"
                tools:layout_editor_absoluteY="47dp" />

            <VideoView
                android:id="@+id/vidView3"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="47dp" />

            <TextView
                android:id="@+id/txtView4"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="30dp"
                android:paddingTop="5dp"
                android:text="Dumbbell Floor Press"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp" />


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView3"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginTop="14dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="30dp"
            app:cardMaxElevation="30dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView2"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp">

            <TextView
                android:id="@+id/txtView6"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="40dp"
                android:paddingTop="5dp"
                android:text="Trap Bar Deadlift"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="206dp"
                tools:layout_editor_absoluteY="16dp" />

            <VideoView
                android:id="@+id/vidView6"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="225dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="218dp"
                tools:layout_editor_absoluteY="47dp" />

            <VideoView
                android:id="@+id/vidView5"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="47dp" />

            <TextView
                android:id="@+id/txtView5"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="30dp"
                android:paddingTop="5dp"
                android:text="Dumbbell Bench Press"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp" />


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginTop="14dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="30dp"
            app:cardMaxElevation="30dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView3"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp">

            <TextView
                android:id="@+id/txtView8"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="45dp"
                android:paddingTop="5dp"
                android:text="Dumbbell Row"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="206dp"
                tools:layout_editor_absoluteY="16dp" />

            <VideoView
                android:id="@+id/vidView8"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="225dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="218dp"
                tools:layout_editor_absoluteY="47dp" />

            <VideoView
                android:id="@+id/vidView7"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="@color/blue"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="47dp" />

            <TextView
                android:id="@+id/txtView7"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp"
                android:paddingLeft="6dp"
                android:paddingTop="5dp"
                android:text="Alternating Dumbbell Bench Press"
                android:textSize="10sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp" />


        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>